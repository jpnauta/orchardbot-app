    "express": "4.15.2",
    "gulp-babel": "6.1.2",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "mongoose": "4.11.6",
    "node-schedule": "1.2.4",
    "promise": "8.0.1",
    "socket.io": "2.0.3",
